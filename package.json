{
    "name":                 "proof",
    "version":              "1.2.4",
    "description":

        "A UNIX way test non-framework for the meticulous programmer who is also very lazy.",

    "keywords":
    [
                            "test", "unit", "tap", "assert", "tdd"
    ],
    "author":               "Alan Gutierrez <alan@prettyrobots.com>",
    "homepage":             "https://bigeasy.github.io/proof",
    "bugs":                 "https://github.com/bigeasy/proof/issues",
    "license":              "MIT",
    "repository":
    {
                            "url": "https://github.com/bigeasy/proof",
                            "type": "git"
    },
    "dependencies":
    {
                            "arguable":                     "4.2.x",
                            "byline":                       "5.0.0",
                            "cadence":                      "1.0.x",
                            "deep-equal":                   "1.0.1",
                            "delta":                        "1.1.x",
                            "departure":                    "0.0.3",
                            "expandable":                   "0.0.6",
                            "reactor":                      "2.2.x",
                            "staccato":                     "1.1.x"
    },
    "devDependencies":
    {
                            "coffee-script":                ">=1.1.3"
    },
    "bin":
    {
                            "proof":                        "proof.bin.js"
    },
    "scripts" :
    {
                            "test": "(node proof.bin.js run t/*/*.t.* t/*/*/*.t.* | tee .proof.out | node proof.bin.js progress) || (node proof.bin.js errors < .proof.out)"
    }
}
